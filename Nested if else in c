#include<stdio.h>

int main()

{

int a;

int b;

printf("Enter value for a:");

scanf("%d",&a);

printf("Enter value for b:");

scanf("%d",&b);

if(a==b)

{
printf("%d is equal to %d",a,b);
}


if(a>b)

{
printf("%d is greater than %d",a,b);
}

else

{

printf("%d is less than %d",a,b);

}

return 0;

}
